<launch>
  <include file="$(find gazebo_ros)/launch/empty_world.launch"> 
    
    <arg
			name="world_name"
			value="$(find hola_task2)/worlds/grid-2.world"
		/>
  </include>
  <node
    name="spawn_model"
    pkg="gazebo_ros"
    type="spawn_model"
    args="-file $(find hola_task2)/urdf/hola_bot.urdf -urdf -model hola_bot "
    launch-prefix="bash -c 'sleep 5; $0 $@' "
    />
</launch>
